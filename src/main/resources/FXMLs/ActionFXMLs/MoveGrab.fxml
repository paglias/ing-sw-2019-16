<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.ActionsControllers.MoveGrabController">
    <children>
        <Pane prefHeight="275.0" prefWidth="452.0">
            <children>
                <Label layoutX="146.0" layoutY="14.0" text="MOVE AND GRAB ACTION" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font size="11.0" />
                    </font>
                    <padding>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </padding>
                </Label>
                <Button fx:id="move" layoutX="58.0" layoutY="125.0" mnemonicParsing="false" onAction="#openMove" prefHeight="25.0" prefWidth="88.0" text="Move" />
                <Button fx:id="grab" layoutX="58.0" layoutY="174.0" mnemonicParsing="false" onAction="#openGrab" prefHeight="25.0" prefWidth="88.0" text="Grab" />
                <Text layoutX="50.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please perform the following actions as needed:" />
            <Button fx:id="confirmButton" layoutX="344.0" layoutY="199.0" mnemonicParsing="false" onAction="#confirmChoice" text="Confirm" />
            </children>
        </Pane>
    </children>
</AnchorPane>
