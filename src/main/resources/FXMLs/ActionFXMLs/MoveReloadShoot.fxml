<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.ActionsControllers.MoveReloadShootController">
    <children>
        <Pane prefHeight="275.0" prefWidth="452.0">
            <children>
                <Label layoutX="126.0" layoutY="14.0" text="MOVE, RELOAD AND SHOOT ACTION" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font size="11.0" />
                    </font>
                    <padding>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </padding>
                </Label>
                <Button fx:id="move" layoutX="65.0" layoutY="100.0" mnemonicParsing="false" onAction="#openMove" prefHeight="25.0" prefWidth="88.0" text="Move" />
                <Button fx:id="reload" layoutX="65.0" layoutY="150.0" mnemonicParsing="false" onAction="#openReload" prefHeight="25.0" prefWidth="88.0" text="Reload" />
                <Button fx:id="shoot" layoutX="65.0" layoutY="200.0" mnemonicParsing="false" onAction="#openShoot" prefHeight="25.0" prefWidth="88.0" text="Shoot" />
                <Text layoutX="65.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please perform the following actions as needed:" />
            <Button fx:id="continueButton" layoutX="339.0" layoutY="200.0" mnemonicParsing="false" onAction="#confirmAction" text="Continue" />
            </children>
        </Pane>
    </children>
</AnchorPane>
